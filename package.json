{
  "name": "serverless-ts-boilerplate",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript and multi micro services",
  "main": "handler.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "layer-create:dev": "mkdir -p src/layers/layer_node-dev && cpx package.json src/layers/layer_node-dev && (cd src/layers/layer_node-dev && npm i --production)",
    "layer-create:prod": "mkdir -p src/layers/layer_node-prod && cpx package.json src/layers/layer_node-prod && (cd src/layers/layer_node-prod && npm i --production)",
    "ts-compile": "node node_modules/typescript/bin/tsc",
    "lint": "node node_modules/tslint/bin/tslint -p ."
  },
  "pre-commit": [
    "ts-compile",
    "lint"
  ],
  "dependencies": {
    "amazon-cognito-identity-js": "1.31.0",
    "currency.js": "1.2.1",
    "http-status-codes": "1.3.0",
    "joi": "14.3.1",
    "jsonwebtoken": "8.4.0",
    "jwk-to-pem": "2.0.1",
    "jwt-decode": "2.2.0",
    "knex": "0.19.5",
    "middy": "0.22.0",
    "middy-middleware-warmup": "1.0.3",
    "mysql2": "1.6.4",
    "ramda": "0.26.1",
    "request": "2.88.0",
    "source-map-support": "0.5.6",
    "uuid": "3.3.2",
    "voucher-code-generator": "1.1.1",
    "xregexp": "4.2.4"
  },
  "devDependencies": {
    "@types/jest": "24.0.0",
    "@types/joi": "14.3.1",
    "@types/jwt-decode": "2.2.1",
    "@types/knex": "0.15.1",
    "@types/node": "8.0.57",
    "@types/ramda": "0.25.50",
    "@types/xregexp": "3.0.29",
    "copy-webpack-plugin": "5.0.0",
    "cpx": "1.5.0",
    "glob": "7.1.3",
    "jest": "24.1.0",
    "pre-commit": "1.2.2",
    "serverless-domain-manager": "2.6.13",
    "serverless-offline": "4.2.0",
    "serverless-plugin-warmup": "4.5.3-rc.1",
    "serverless-pseudo-parameters": "2.4.0",
    "serverless-webpack": "5.1.1",
    "ts-loader": "4.2.0",
    "tslint": "5.12.1",
    "typescript": "3.2.4",
    "webpack": "4.5.0",
    "webpack-node-externals": "1.7.2"
  },
  "author": "KMNowak",
  "license": "MIT"
}
